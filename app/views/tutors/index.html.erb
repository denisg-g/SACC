<p id="notice"><%= notice %></p>

<h1>Listing Tutors</h1>

<table>
  <thead>
    <tr>
      <th>Tipo</th>
      <th>Id</th>
      <th>First</th>
      <th>Second</th>
      <th>Apellido</th>
      <th>Lastname</th>
      <th>Tel tutor</th>
      <th>Tipo tutor</th>
      <th>Email</th>
      <th>User</th>
      <th>Direction</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tutors.each do |tutor| %>
      <tr>
        <td><%= tutor.tipo_id %></td>
        <td><%= tutor.id %></td>
        <td><%= tutor.first %></td>
        <td><%= tutor.second %></td>
        <td><%= tutor.apellido %></td>
        <td><%= tutor.lastname %></td>
        <td><%= tutor.tel_tutor_id %></td>
        <td><%= tutor.tipo_tutor %></td>
        <td><%= tutor.email %></td>
        <td><%= tutor.user_id %></td>
        <td><%= tutor.direction %></td>
        <td><%= link_to 'Show', tutor %></td>
        <td><%= link_to 'Edit', edit_tutor_path(tutor) %></td>
        <td><%= link_to 'Destroy', tutor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tutor', new_tutor_path %>
